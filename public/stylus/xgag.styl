/*-------------------------------------
	xgag header
------------------------------------- */

	#xgag-header{
		border-bottom: 1px solid #ddd;
		max-height: 100%;
	}
	#xgag-header.affix{
		width: 100%;
		z-index: 10;
		top: 0;
	}
	.admin-bar #xgag-header.affix {
		top: 0;
		z-index: 99;
	}
	@media (min-width: 768px){

		.admin-bar #xgag-header.affix {
			top: 32px;
		}
		#xgag-header{
			border-bottom: none;
		}
	}
	@media (max-width: 992px){
		#xgag-header{
			overflow-y: hidden;
		}
	}

	.post-vote-header .vote-count{
		padding: 7px 12px;
		font-weight: bold;
	}

	.header-actions > a {
		width: 100%;
		margin-right: 10px;
	}
	.header-actions .navbar-form input[type="text"] {
		max-width: 100px;
	}
	@media (min-width: 768px){
		.single #xgag-header.affix .header-actions{
			opacity: 0;
		}
		.header-actions > a {
			width: auto;
		}
		.header-actions .navbar-form input[type="text"] {
			max-width: 150px;
		}
	}
	@media (min-width: 992px){


		.header-actions .navbar-form input[type="text"] {
			max-width: 100%;
		}
	}
	.single #xgag-header .header-actions {
		opacity: 1;
		-moz-transition: opacity 0.2s ease 0s;
		-o-transition: opacity 0.2s ease 0s;
		-webkit-transition: opacity 0.2s ease 0s;
		transition: opacity 0.2s ease 0s;
	}



